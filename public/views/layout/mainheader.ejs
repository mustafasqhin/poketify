<style nonce="<%=hash%>">
  /* latin */
  @font-face {
    font-family: 'Comic';
    src: url('/fonts/comic-webfont.woff2') format('woff2');
  }

  * {
    padding: 0;
    margin: 0;
    box-sizing: border-box;
    font-family: 'Comic';
  }

  .preload {
    width: 100vw;
    height: 100vh;
    background: var(--primary-dark);
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    z-index: 1000;
    position: fixed;
    top: 0;
    left: 0;
    gap: 10px;
  }

  .preload .logo {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
  }

  .preload .text {
    padding: 25px;
    color: var(--secondary-dark);
  }

  .preload img {
    display: inline-block;
    z-index: 1000;
    animation: rotate 700ms infinite;
  }

  .preload .preload-ripple {
    width: 100px;
    height: 100px;
    background: #ffffff3f;
    border-radius: 50%;
    position: absolute;
    animation: preload-ripple 700ms infinite;
    z-index: 999;
  }

  .preload.done {
    display: none;
  }

  @keyframes preload-ripple {
    0% {
      height: 50px;
      width: 50px;
      opacity: 1;
    }

    100% {
      height: 100px;
      width: 100px;
      opacity: 0;
    }
  }

  @keyframes rotate {
    0% {
      transform: rotate(0deg);
    }

    80% {
      transform: rotate(360deg);
    }

    100% {
      transform: rotate(360deg);
    }
  }
</style>

